  "flows": [
    {
      "seed_key": "admin-ui-demo-flow",
      "name": "Admin UI Demo Flow",
      "description": "Simple demo flow for admin UI testing.",
      "version": "1.0.0",
      "entry_node_id": "welcome",
      "visibility": "school",
      "school_seed_key": "admin-ui-school",
      "flow_data": {
        "nodes": [
          {
            "id": "welcome",
            "type": "message",
            "content": {
              "messages": [
                {
                  "type": "text",
                  "text": "Welcome to the Huey Books demo flow!"
                }
              ]
            },
            "position": {"x": 200, "y": 100}
          },
          {
            "id": "ask_genre",
            "type": "question",
            "content": {
              "question": {"text": "Pick a genre you like:"},
              "input_type": "choice",
              "options": [
                {"label": "Fantasy", "value": "fantasy"},
                {"label": "Mystery", "value": "mystery"}
              ],
              "variable": "temp.genre"
            },
            "position": {"x": 450, "y": 100}
          },
          {
            "id": "genre_branch",
            "type": "condition",
            "content": {
              "conditions": [
                {"if": "temp.genre == 'fantasy'", "then": "$0"},
                {"if": "temp.genre == 'mystery'", "then": "$1"}
              ],
              "default_path": "default"
            },
            "position": {"x": 700, "y": 100}
          },
          {
            "id": "fantasy_msg",
            "type": "message",
            "content": {
              "messages": [
                {
                  "type": "text",
                  "text": "Great choice! Fantasy stories are full of magic and adventure."
                }
              ]
            },
            "position": {"x": 950, "y": 40}
          },
          {
            "id": "mystery_msg",
            "type": "message",
            "content": {
              "messages": [
                {
                  "type": "text",
                  "text": "Mysteries are the best! Time to crack the case."
                }
              ]
            },
            "position": {"x": 950, "y": 140}
          },
          {
            "id": "default_msg",
            "type": "message",
            "content": {
              "messages": [
                {
                  "type": "text",
                  "text": "Thanks for sharing! Let's find some great books."
                }
              ]
            },
            "position": {"x": 950, "y": 240}
          }
        ],
        "connections": [
          {"source": "welcome", "target": "ask_genre", "type": "default"},
          {"source": "ask_genre", "target": "genre_branch", "type": "default"},
          {"source": "genre_branch", "target": "fantasy_msg", "type": "$0"},
          {"source": "genre_branch", "target": "mystery_msg", "type": "$1"},
          {"source": "genre_branch", "target": "default_msg", "type": "default"}
        ]
      }
    },
    {"flow_file": "huey-profile-flow.json"},
    {"flow_file": "huey-preferences-flow.json"},
    {"flow_file": "huey-recommendation-flow.json"},
    {
      "flow_file": "huey-bookbot-flow.json",
      "theme_seed_key": "huey-bookbot-theme"
    }
  ]
