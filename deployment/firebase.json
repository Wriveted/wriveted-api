{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/admin/**",
        "run": {
          "serviceId": "wriveted-admin-ui",
          "region": "australia-southeast1"
        }
      },
      {
        "source": "/v1/**",
        "run": {
          "serviceId": "wriveted-api",
          "region": "australia-southeast1"
        }
      }
    ]
  }
}
